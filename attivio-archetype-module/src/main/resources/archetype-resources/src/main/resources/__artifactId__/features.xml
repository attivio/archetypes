<att:attivio xmlns:att="http://www.attivio.com/configuration"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:f="http://www.attivio.com/configuration/features/core"
	xmlns:util="http://www.springframework.org/schema/util" 
	xsi:schemaLocation="http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd 
						http://www.attivio.com/configuration/features/core http://www.attivio.com/configuration/features/coreFeatures.xsd 
						http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd 
						http://www.attivio.com/configuration http://www.attivio.com/configuration/attivio-config.xsd ">

  <features>
    <f:deployWebapp directory="webapps/${artifactId}" context-path="/${artifactId}" nodeset="*">
      <f:menuEntry label="${artifactId} webapp" path="tools" />
    </f:deployWebapp>
  </features>
	
</att:attivio>
